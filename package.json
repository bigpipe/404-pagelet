{
  "name": "404-pagelet",
  "version": "1.0.5",
  "description": "Default 404 Pagelet used if the router cannot dispatch to any route",
  "main": "index.js",
  "scripts": {
    "test": "mocha $(find test -name '*.test.js')",
    "watch": "mocha --watch $(find test -name '*.test.js')",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- $(find test -name '*.test.js')",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- $(find test -name '*.test.js')"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bigpipe/404-pagelet.git"
  },
  "dependencies": {
    "pagelet": "0.9.x",
    "diagnostics-pagelet": "1.0.x"
  },
  "devDependencies": {
    "assume": "~1.4.1",
    "istanbul": "~0.4.5",
    "mocha": "~3.1.2",
    "pre-commit": "~1.1.3",
    "routable": "0.0.x",
    "temper": "~0.3.2"
  },
  "keywords": [
    "Pagelet",
    "BigPipe",
    "404",
    "default",
    "not",
    "found"
  ],
  "author": "Martijn Swaagman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigpipe/404-pagelet/issues"
  },
  "homepage": "https://github.com/bigpipe/404-pagelet"
}
